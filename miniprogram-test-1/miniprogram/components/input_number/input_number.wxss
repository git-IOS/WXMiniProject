.num-box{
  display: flex;
  width: 225rpx;
  height: 58rpx;
  line-height: 58rpx;
  border: 1rpx solid #cfcfcf;
  border-radius: 8rpx;
  position: relative;
  z-index: 0;
  align-items: center;
  margin-top: 1rpx;
}
.num-box .num-input{
  width: 88rpx;
  height: 56rpx;
  line-height: 56rpx;
  border-left: 2rpx solid #cfcfcf;
  border-right: 2rpx solid #cfcfcf;
  color: #333;
  text-align: center;
}
.num-box .num-box-btn{
  flex: 1;
  text-align: center;
  font-size: 50rpx;
  color: #333;
  padding: 0;
}
.num-box .num-box-btn.disabled{
  color: #cfcfcf;
}
