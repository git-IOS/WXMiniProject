
/*中央悬浮loading的相关样式*/
.loading-img {
    width: 64rpx;
    height: 64rpx;
    background: url('data:image/png;base64,') no-repeat;
    background-size: 100% 100%;
    position: absolute;
    animation: loading 1.2s linear 0s infinite;
    left: 0;
    top: 0;
}

.pay_success_icon {
    width: 64rpx;
    height: 64rpx;
    border-radius: 64rpx;
    background-color: white;
    position: absolute;
    left: 0;
    top: 0;
    margin-left:73rpx;
    margin-top:51rpx;
}

.pay_success_icon:before {
    content: "\e623";
    color: black;
    font-family: icomoon;
    font-size:28rpx;
    line-height: 64rpx;
}

.loading-wrap {
    position: fixed;
    width: 118rpx;
    height: 118rpx;
    left: 50%;
    top: 38%;
    z-index: 99999;
    text-align: center;
    margin-left: -59rpx;
    background-color: rgba(255,255,255,0.9);
    border-radius: 35rpx;
}

/*免密支付样式*/
.pay-loading-wrap {
    width:210rpx;
    height:210rpx;
    background-color:rgba(00, 00, 00, .8);
    border-radius:20rpx;
    left:50%;
    margin-left:-105rpx;
    margin-top:-23rpx;
}

.loading-wrap .loading-img{
    float: left;
    margin-left: 28rpx;
    margin-top: 28rpx;
}

/*免密支付样式-loading img 调整*/
.pay-loading-wrap .loading-img{
    margin-left:73rpx;
    margin-top: 51rpx;
}

/*免密支付样式-tips*/
.pay-loading-wrap .tips {
    font-size: 30rpx;
    color: white;
    line-height: 1;
    position: absolute;
    bottom: 40rpx;
    width:100%;
}

.loading-top {
    top: 20%;
}

@keyframes loading {
    0% {
        transform: rotate(0);
        -webkit-transform: rotate(0);
    }
    100% {
        transform: rotate(360deg);
        -webkit-transform: rotate(360deg);
    }
}

@-webkit-keyframes loading {
    0% {
        transform: rotate(0);
        -webkit-transform: rotate(0);
    }
    100% {
        transform: rotate(360deg);
        -webkit-transform: rotate(360deg);
    }
}

